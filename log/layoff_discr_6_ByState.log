 13.         local t_h _b[is_hisp]/_se[is_hisp] 
 14. 
.         * get p values
.         local p_f = 2*ttail(e(df_r),abs(`t_f'))
 15.         local p_p = 2*ttail(e(df_r),abs(`t_p'))
 16.         local p_a = 2*ttail(e(df_r),abs(`t_a'))
 17.         local p_n = 2*ttail(e(df_r),abs(`t_n'))
 18.         local p_h = 2*ttail(e(df_r),abs(`t_h'))
 19.         
.         post `results' ///
>         (`Enum') ///
>         (_b[is_female]) (_b[is_poc]) (_b[is_asian]) (_b[is_native]) (_b[is_hisp]) /// 
>         (`p_f') (`p_p') (`p_a') (`p_n') (`p_h')
 20.         
.         * restore and re-initiate loop
.         restore
 21.         local Enum = `Enum' + 1
 22. }
